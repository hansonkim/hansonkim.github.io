  },
  "homepage": "https://github.com/hansonkim/hansonkim.github.io#readme",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
